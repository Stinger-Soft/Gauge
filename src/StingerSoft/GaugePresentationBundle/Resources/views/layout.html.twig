{% trans_default_domain 'StingerSoftGaugePresentationBundle' %}
<!DOCTYPE html>
<html lang=en>
<head>
<meta charset=utf-8>
<meta http-equiv=X-UA-Compatible content="IE=edge">
<meta name=viewport content="width=device-width,initial-scale=1">
<meta name=author content="Oliver Kotte, Florian Meyer">
<title>{% block title %}{% endblock %}</title>


{% block stylesheets %}

{% stylesheets filter="cssrewrite"
	'bundles/bmatznerfontawesome/css/font-awesome.min.css'
	'bundles/stingersoftplatform/scss/bootstrap.scss'
	'bundles/stingersoftplatform/scss/components.scss'
	'bundles/stingersoftgaugepresentation/scss/layout.scss'
%}
	<link rel="stylesheet" type="text/css" href="{{ asset_url }}" />
{% endstylesheets %}

{% endblock stylesheets %}

{% block javascripts_head %}
    {% javascripts
        '@header_js' 
    %}
        <script src="{{ asset_url }}"></script>
    {% endjavascripts %}
{% endblock javascripts_head %}
</head>
<body>
{% block body %}
	<header class="navbar navbar-inverse navbar-static-top" id="top">
		<div class="container-fluid">
			<div class="navbar-header">
				<button class="navbar-toggle collapsed" type="button"
					data-toggle="collapse" data-target="#bs-navbar"
					aria-controls="bs-navbar" aria-expanded="false">
					<span class="sr-only">{{ 'stinger_soft_gauge.layout.toggle_navigation'|trans }}</span>
						<span class="icon-bar"></span>
						<span class="icon-bar"></span>
						<span class="icon-bar"></span>
				</button>
				<a href="/" class="navbar-brand">
					{% image '@StingerSoftGaugePresentationBundle/Resources/public/img/brand.png' %}
						<img src="{{ asset_url }}" alt="{{ 'stinger_soft_gauge_presentation.layout.brand'|trans }}" />
					{% endimage %}
				</a>
			</div>
			<nav id="bs-navbar" class="collapse navbar-collapse">
				<ul class="nav navbar-nav">
					<li class="active">
						<a href="{{ path('stinger_soft_gauge_presentation_homepage') }}">
							<i class="fa fa-fw fa-home"></i>
							{{ 'stinger_soft_gauge_presentation.layout.menu.presentations'|trans }}
						</a>
					</li>
					<li>
						<a href="/">
							<i class="fa fa-fw fa-paint-brush"></i>
							{{ 'stinger_soft_gauge_presentation.layout.menu.styles'|trans }}
						</a>
					</li>
					<li>
						<a href="/">
							<i class="fa fa-fw fa-cog"></i>
							{{ 'stinger_soft_gauge_presentation.layout.menu.settings'|trans }}
						</a>
					</li>
				</ul>
			</nav>
		</div>
	</header>
	<section id="body">
		{% block content %}
		<div class="container-fluid">
		
		</div>
		{% endblock content %}
	</section>
{% endblock body %}
{% block javascripts_body %}
{% endblock javascripts_body %}
</body>
</html>